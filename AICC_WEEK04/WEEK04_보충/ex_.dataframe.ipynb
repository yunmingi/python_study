    "df = pd.DataFrame(data, index)\n",
    "\n",
    "#iloc 활용\n",
    "print(df.iloc[0:2,0:2])\n",
    "#loc 활용\n",
    "print(df.loc[\"재욱\":\"철수\",\"국어\":\"수학\"])"
